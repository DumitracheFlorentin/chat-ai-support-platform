generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  price       Float?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model ApiKey {
  id         Int      @id @default(autoincrement())
  key        String   @unique
  createdAt  DateTime @default(now())
  expiresAt  DateTime
  owner      String?
  active     Boolean  @default(true)
}
